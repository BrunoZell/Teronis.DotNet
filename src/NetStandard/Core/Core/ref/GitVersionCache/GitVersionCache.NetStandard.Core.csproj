<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>

    <SourceProjectDirectory>$(MSBuildThisFileDirectory)..\..\src\</SourceProjectDirectory>

    <DisableGitVersion>true</DisableGitVersion>
    <DisableGitVersionCache>true</DisableGitVersionCache>
    <DisableLocalizationProject>true</DisableLocalizationProject>
    <DisableSdkProps>true</DisableSdkProps>

  </PropertyGroup>

  <Import Project="$(TeronisMSBuildCoreProjectShadowTargetsFile)" />
  <Import Project="$(SourceProjectDirectory)Teronis.NetStandard.Core.csproj" />

  <ItemGroup>
    <ProjectReference Include="..\..\..\Core.Localization\ref\GitVersionCache\GitVersionCache.NetStandard.Core.Localization.csproj" PrivateAssets="all" />
  </ItemGroup>

</Project>
