    <TeronisMSBuildPackagingUnzipPackageTargets>$(RootDirectory)src\MSBuild\Core\Core\src\bin\PackagePublish\build\Teronis.MSBuild.Packaging.UnzipPackage.targets</TeronisMSBuildPackagingUnzipPackageTargets>
    <TeronisMSBuildPackagingGenerateFilesystemPackageTargets>$(RootDirectory)src\MSBuild\Core\Core\src\bin\PackagePublish\build\Teronis.MSBuild.Packaging.GenerateFilesystemPackage.targets</TeronisMSBuildPackagingGenerateFilesystemPackageTargets>
    <TeronisMSBuildCoreTasksTargets>$(RootDirectory)src\MSBuild\Core\Core\src\bin\PackagePublish\build\Teronis.MSBuild.Tasks.targets</TeronisMSBuildCoreTasksTargets>
    <TeronisMSBuildCoreUtilitiesTargets>$(RootDirectory)src\MSBuild\Core\Core\src\bin\PackagePublish\build\Teronis.MSBuild.Utilities.targets</TeronisMSBuildCoreUtilitiesTargets>
